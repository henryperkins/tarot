{
  "expo": {
    "name": "Tableu: Ritual Tarot Reader",
    "slug": "tableu-native",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "tableu",
    "userInterfaceStyle": "dark",
    "sdkVersion": "55.0.0",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "description": "A scholarly tarot reflection system with deterministic ritual draws, knowledge graph-powered interpretations, and archetypal pattern detection. Not fortune-tellingâ€”structured self-reflection grounded in tarot scholarship.",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hperkins.tableau.native",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "privacyInfo": {
        "NSPrivacyTracking": false,
        "NSPrivacyCollectedDataTypes": [],
        "NSPrivacyAccessedAPITypes": []
      }
    },
    "android": {
      "package": "com.hperkins.tableau.native"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-build-properties",
      "expo-sqlite",
      "expo-image"
    ]
  }
}
